<?xml version="1.0" encoding="UTF-8"?>
<clientConfig version="1.1">
  <emailProvider id="catalystapp.co">
    <domain>catalystapp.co</domain>
    <displayName>Catalyst</displayName>
    <displayShortName>Catalyst</displayShortName>
    <incomingServer type="imap">
      <hostname>catalystapp.co</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <authentication>password-cleartext</authentication>
      <username>%EMAILADDRESS%</username>
    </incomingServer>
    <outgoingServer type="smtp">
      <hostname>smtp.catalystapp.co</hostname>
      <port>587</port>
      <socketType>SSL</socketType>
      <authentication>password-cleartext</authentication>
      <username>%EMAILADDRESS%</username>
    </outgoingServer>
    <documentation url="https://admin.catalystapp.co/email"/>
  </emailProvider>
</clientConfig>
